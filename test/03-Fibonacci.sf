
BEGINPROG Fibonacci

/* Compute the xth Fibonacci number
   If the input number is negative, print -1. */

VARIABLES x, b, c, temp    /*  */
  READ(x)              // Read a number from user input

  b := 0
  c := 1
  IF (x < 0) THEN
    PRINT(-1)
  ELSE
    WHILE (x > 0) DO
      temp := b + c
      b := c
      c := temp
      x := x - 1
    ENDWHILE
    PRINT(b)
  ENDIF
ENDPROG